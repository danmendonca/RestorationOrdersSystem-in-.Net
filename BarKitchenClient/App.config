<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <system.runtime.remoting>
    <application>
      <client>
        <wellknown
          type="SingleServer, Server"
          url="tcp://localhost:9000/Server/SingleServer"
         />
      </client>
      <channels>
        <channel ref="tcp client"/>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>